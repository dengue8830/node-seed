{
  "apps": [
    {
      "name": "node-seed",
      "script": "./dist/server.js",
      "env": {
        "NODE_ENV": "development"
      },
      "env_development": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "instances": "1",
      "exec_mode": "cluster",
      "instance_var": "INSTANCE_ID",
      "source_map_support": true,
      "max_restarts": 1,
      "min_uptime": "3s"
    }
  ]
}